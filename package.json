{
  "name": "wmapp",
  "version": "1.0.0",
  "description": "Warehouse Manegement application",
  "repository": "https://github.com/svolochj/WMapp-CAP.git",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5.6.2",
    "@sap/hana-client": "^2.10.15",
    "@sap/hdbext": "^7.5.0",
    "express": "^4",
    "passport": "^0.5.0",
    "sap-hdbext-promisfied": "^2.202201.1"
  },
  "devDependencies": {
    "@sap/hdi-deploy": "^4.2.3",
    "@sap/ux-specification": "^1.96.2",
    "jest": "^26.6.3",
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "cds run",
    "test": "jest",
    "undeploy": "cf undeploy wmapp --delete-services --delete-service-keys"
  },
  "cds": {
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana"
      },
      "uaa": {
        "kind": "xsuaa",
        "credentials": {}
      }
    }
  },
  "sapux": [
    "app/cat/uom",
    "app/cat/locations",
    "app/cat/products",
    "app/doc/receive",
    "app/doc/movement",
    "app/doc/issue",
    "app/reqmovement"
  ]
}
